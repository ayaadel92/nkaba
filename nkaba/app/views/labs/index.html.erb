<p id="notice"><%= notice %></p>

<h1>Listing Labs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Governorate</th>
      <th>Latitude</th>
      <th>Longitudes</th>
      <th>Area</th>
      <th>Rate</th>
      <th>Descriptation</th>
      <th>Path</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @labs.each do |lab| %>
      <tr>
        <td><%= lab.name %></td>
        <td><%= lab.address %></td>
        <td><%= lab.phone %></td>
        <td><%= lab.governorate %></td>
        <td><%= lab.latitude %></td>
        <td><%= lab.longitudes %></td>
        <td><%= lab.area %></td>
        <td><%= lab.rate %></td>
        <td><%= lab.descriptation %></td>
        <td><%= lab.path %></td>
        <td><%= lab.type %></td>
        <td><%= link_to 'Show', lab %></td>
        <td><%= link_to 'Edit', edit_lab_path(lab) %></td>
        <td><%= link_to 'Destroy', lab, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lab', new_lab_path %>
