<p id="notice"><%= notice %></p>

<h1>Listing Doctors</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Degree</th>
      <th>Specialization</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Governorate</th>
      <th>Area</th>
      <th>Price</th>
      <th>Time from</th>
      <th>Time to</th>
      <th>Mobile</th>
      <th>Rate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doctors.each do |doctor| %>
      <tr>
        <td><%= doctor.name %></td>
        <td><%= doctor.degree %></td>
        <td><%= doctor.specialization %></td>
        <td><%= doctor.address %></td>
        <td><%= doctor.phone %></td>
        <td><%= doctor.governorate %></td>
        <td><%= doctor.area %></td>
        <td><%= doctor.price %></td>
        <td><%= doctor.time_from %></td>
        <td><%= doctor.time_to %></td>
        <td><%= doctor.mobile %></td>
        <td><%= doctor.rate %></td>
        <td><%= link_to 'Show', doctor %></td>
        <td><%= link_to 'Edit', edit_doctor_path(doctor) %></td>
        <td><%= link_to 'Destroy', doctor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doctor', new_doctor_path %>
