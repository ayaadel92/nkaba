<p id="notice"><%= notice %></p>

<h1>Listing Engineers</h1>

<table>
  <thead>
    <tr>
      <th>Membersh number</th>
      <th>Number health care</th>
      <th>National</th>
      <th>Name</th>
      <th>Address</th>
      <th>Phone number</th>
      <th>Data of birth</th>
      <th>Join data</th>
      <th>Graduation year</th>
      <th>Number of participants</th>
      <th>Gender</th>
      <th>Credit card</th>
      <th>Email</th>
      <th>Path</th>
      <th>Limit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @engineers.each do |engineer| %>
      <tr>
        <td><%= engineer.membersh_number %></td>
        <td><%= engineer.number_health_care %></td>
        <td><%= engineer.national_id %></td>
        <td><%= engineer.name %></td>
        <td><%= engineer.address %></td>
        <td><%= engineer.phone_number %></td>
        <td><%= engineer.data_of_birth %></td>
        <td><%= engineer.join_data %></td>
        <td><%= engineer.graduation_year %></td>
        <td><%= engineer.number_of_participants %></td>
        <td><%= engineer.gender %></td>
        <td><%= engineer.credit_card %></td>
        <td><%= engineer.email %></td>
        <td><%= engineer.path %></td>
        <td><%= engineer.limit %></td>
        <td><%= link_to 'Show', engineer %></td>
        <td><%= link_to 'Edit', edit_engineer_path(engineer) %></td>
        <td><%= link_to 'Destroy', engineer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Engineer', new_engineer_path %>
