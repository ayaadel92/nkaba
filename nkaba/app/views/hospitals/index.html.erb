<p id="notice"><%= notice %></p>

<h1>Listing Hospitals</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Governorate</th>
      <th>Latitude</th>
      <th>Longitudes</th>
      <th>Area</th>
      <th>Rate</th>
      <th>Descriptation</th>
      <th>Path</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hospitals.each do |hospital| %>
      <tr>
        <td><%= hospital.name %></td>
        <td><%= hospital.address %></td>
        <td><%= hospital.phone %></td>
        <td><%= hospital.governorate %></td>
        <td><%= hospital.latitude %></td>
        <td><%= hospital.longitudes %></td>
        <td><%= hospital.area %></td>
        <td><%= hospital.rate %></td>
        <td><%= hospital.descriptation %></td>
        <td><%= hospital.path %></td>
        <td><%= link_to 'Show', hospital %></td>
        <td><%= link_to 'Edit', edit_hospital_path(hospital) %></td>
        <td><%= link_to 'Destroy', hospital, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hospital', new_hospital_path %>
